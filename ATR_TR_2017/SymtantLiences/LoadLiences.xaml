<dx:DXWindow x:Class="SymtantLiences.LoadLiences"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             mc:Ignorable="d"
             Icon="/SymtantLiences;component/images/背景.ico"
             WindowStartupLocation="CenterScreen" ResizeMode="NoResize" 
             Height="200" Width="300" Title="单击版授权">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Name="lblMsg" x:FieldModifier="public"/>
        <TextBox Margin="0,10,0,0" Grid.Row="1" Name="txtLiences" TextWrapping="Wrap" x:FieldModifier="public" AcceptsReturn="True"/>
    </Grid>
</dx:DXWindow>
